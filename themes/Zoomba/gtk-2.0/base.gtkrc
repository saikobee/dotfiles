style "pidgin-status-box" {
    bg[NORMAL]   = @bg_color
    bg[PRELIGHT] = shade(1.05, @bg_color)
    bg[ACTIVE]   = shade(0.95, @bg_color)

    engine "murrine" {
        contrast = 0.50
        highlight_shade = 1.00
    }
}
widget "*pidgin_blist_statusbox*" style: highest "pidgin-status-box"

style "dark-scrollbar" = "dark" {
    GtkScrollbar::trough-side-details = 1
    GtkRange::trough-border = 0
    GtkRange::trough-under-steppers = 1

    bg[SELECTED]    = "#111"
    bg[SELECTED]    = @selected_bg_color
    bg[SELECTED]    = shade(1.15, @bg_color)

    bg[NORMAL]      = shade(0.50, @bg_color)
    bg[PRELIGHT]    = shade(1.20, @bg_color)
    bg[ACTIVE]      = shade(0.50, @bg_color)

    fg[INSENSITIVE] = shade(0.65, @bg_color)
    fg[NORMAL]      = shade(0.50, @bg_color)
    fg[PRELIGHT]    = shade(1.50, @bg_color)
    fg[ACTIVE]      = shade(0.65, @bg_color)

    engine "murrine" {
        roundness = 0
        contrast = 0.20
    }
}
widget "*Moz*croll*ar*" style: highest "dark-scrollbar"

style "square-thin" {
    xthickness = 0
    ythickness = 0

    engine "murrine" {
        roundness = 4
        animation = FALSE
