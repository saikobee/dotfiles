# vim: set syn=gtkrc:

style "entry" = "wider" {
    bg[NORMAL]        = shade(1.00, @base_color)
    base[NORMAL]      = shade(1.00, @base_color)
    text[INSENSITIVE] = shade(0.70, @base_color)

    engine "murrine" {
        roundness = 0
        contrast = 0.50
        focus_color = shade(0.50, @bg_color)
    }
}

